def principal(x,y):
  b = 2*(x**2) + (5*y)**2
  c = -100*x + y**3
  r = (3*x)**2 + y**2

  if r > b and    r > c:
    return ('Rafael ganhou')
  elif b > r and  b > c:
    return ('Beto ganhou') 
  else:
    return ('Carlos ganhou')

n = int(input()) 

for i in range(n):
  x, y = input().split()
  x = int(x)
  y = int(y)

  print (principal(x,y)) 
