numeros = input()
A, B, C = numeros.split()
A = float(A)
B = float(B)
C = float (C)
delta = (B**2)-(4*A*C)
raiz = delta**(1/2) 
if (A == 0):
  print ('Impossivel calcular')
elif (delta < 0 ):
  print ('Impossivel calcular')
else:
  x1 = (- B + raiz) / (2*A)
  x2 = (- B - raiz) / (2*A)
  print ("R1 = %.5f" % (x1))
  print ("R2 = %.5f" % (x2))
